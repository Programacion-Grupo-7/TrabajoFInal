@using WebApplication1.Models;
@model Cancion

@{
    ViewBag.Title = "Home Page";
}

<h1>Tal vez te interese</h1>
@{
    if (ViewBag.ListaCanciones != null)
    {
        foreach (Cancion c in ViewBag.ListaCanciones)
        {
            <label>@c.Nombre</label>
            <audio src="~/Musica/@c.Ubicacion" controls loop></audio>

        }
    }
}

